public class SwichTest {
	
	public static void main(String[] args) {
	 System.out.println("숫자 입력 : ");
	 
	 //사용자 입력
	 Scanner scan = new Scanner (System.in);
	 int number = scan.nextInt();
	 
	 System.out.println(" 입력한 숫자 : " + number);
	}
 
}
