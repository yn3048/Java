package sub1;


/*
 * 날짜: 2023/12/27
 * 이름: 이예나
 * 내용: Java 조건문 if 실습하기
 */
public class IfTest {
	
	public static void main(String[] args) {
		
		//if
		int num1 = 1;
		int num2 = 2;
		
		if(num1 < num2) {
			System.out.println("num1은 num2보다 작다.");
		}
		
		if(num1 > 1) {
			System.out.println("num1은 1보다 크다.");
		}
		
		
		
		//if - else
		
		
		//if - else if - else
	}

}
