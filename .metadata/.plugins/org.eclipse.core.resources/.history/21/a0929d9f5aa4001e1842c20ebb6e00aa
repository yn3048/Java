package sub1;


/*
 * 날짜: 2023/12/27
 * 이름: 이예나
 * 내용: Java 조건문 if 실습하기
 */
public class IfTest {
	
	public static void main(String[] args) {
		
		//if
		int num1 = 1;
		int num2 = 2;
		
		if(num1 < num2) {
			System.out.println("num1은 num2보다 작다.");
		}
		
		if(num1 > 1) {
			System.out.println("num1은 1보다 크다.");
		}
		
		if(num1 > 0) {
			
			if(num2 > 1) {
				System.out.println("num1은 0보다 크고 num2는 1보다 크다");
			}
	
		}
		
		if(num1 > 0 && num2 > 1) {
			System.out.println("num1은 0보다 크고 num2는 1보다 크다");
			}
		
		

		//if - else
		int var1 = 1, var2 = 2;
		if(var1 > var2) {
			//조건이 참일 때
			System.out.println("var1이 var2보다 크다.");
		}else
	
	
		//if - else if - else
		} else {
		
}
		
