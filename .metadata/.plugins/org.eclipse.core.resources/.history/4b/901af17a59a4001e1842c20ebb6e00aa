package sub3;

/*
 * 날짜 : 2023/12/27
 * 이름 : 이예나
 * 내용 : Java 반복문 for 실습하기
 */
public class ForText {

	public static void main(String[] args) {
		//for
		for(int i=1 ; i<=5 ; i++) {
			
			System.out.println("1 : " + i);
		
		}
		
		// 1부터 10까지 합
		int sum = 0;
		
		for (int k=1 ; k<=10 ;  k++) {
			sum = sum + k;
		}	
		System.out.println("1부터 10까지의 합 : " + sum);
		// 1부터 10까지 짝수합
		// for 총점
		// 구구단
		//
	}
}
