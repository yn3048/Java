package sub1;

/*
 * 날짜:2023/12/26
 * 이름:이예나
 * 내용:Ch02. Java 변수와 연산자
 */

public class VariableTest {
	
	public static void main(String[] args) {
	
		//변수
		int a = 3;
		int b = 7;
		String c = "홍길동";
		
		System.out.println("a:" + a);
		System.out.println("b:" + b);
		System.out.println("c:" + c);
		
		a = 5;
		System.out.println("a:" + a);
		
		//상수
		int num = 5;
		num = 7;
		
		System.out.println("num: " + num);
	}
}
