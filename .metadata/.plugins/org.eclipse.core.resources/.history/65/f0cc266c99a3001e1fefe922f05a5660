public class Hello {
	
	public static void main(String[] args) {
		
		//기본출력
		System.out.println("Hello World");
		System.out.println("Hello Java");
		
		//한줄출력
		System.out.print("Hello");
		System.out.print("korea");
		System.out.print("Busan");
	}
}
